#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/uvu@0.5.3/node_modules/uvu/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/uvu@0.5.3/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/node_modules:/Users/lars.heinitz/Desktop/IPA2022/node_modules:/Users/lars.heinitz/Desktop/node_modules:/Users/lars.heinitz/node_modules:/Users/node_modules:/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/mdast-util-from-markdown@1.2.0/node_modules/mdast-util-from-markdown/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/mdast-util-from-markdown@1.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/uvu@0.5.3/node_modules/uvu/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/uvu@0.5.3/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/node_modules:/Users/lars.heinitz/Desktop/IPA2022/node_modules:/Users/lars.heinitz/Desktop/node_modules:/Users/lars.heinitz/node_modules:/Users/node_modules:/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/mdast-util-from-markdown@1.2.0/node_modules/mdast-util-from-markdown/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/mdast-util-from-markdown@1.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uvu@0.5.3/node_modules/uvu/bin.js" "$@"
else
  exec node  "$basedir/../../../../../uvu@0.5.3/node_modules/uvu/bin.js" "$@"
