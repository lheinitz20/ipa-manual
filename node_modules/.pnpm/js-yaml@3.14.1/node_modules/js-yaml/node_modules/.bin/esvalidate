#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/node_modules:/Users/lars.heinitz/Desktop/IPA2022/node_modules:/Users/lars.heinitz/Desktop/node_modules:/Users/lars.heinitz/node_modules:/Users/node_modules:/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/js-yaml@3.14.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/node_modules:/Users/lars.heinitz/Desktop/IPA2022/node_modules:/Users/lars.heinitz/Desktop/node_modules:/Users/lars.heinitz/node_modules:/Users/node_modules:/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/js-yaml@3.14.1/node_modules"
