else
  export NODE_PATH="$NODE_PATH:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/bin/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/nanoid@3.1.30/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/node_modules:/Users/lars.heinitz/Desktop/IPA2022/node_modules:/Users/lars.heinitz/Desktop/node_modules:/Users/lars.heinitz/node_modules:/Users/node_modules:/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/postcss@8.4.5/node_modules/postcss/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/postcss@8.4.5/node_modules"
