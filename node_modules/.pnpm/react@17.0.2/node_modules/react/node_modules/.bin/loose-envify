#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/node_modules:/Users/lars.heinitz/Desktop/IPA2022/node_modules:/Users/lars.heinitz/Desktop/node_modules:/Users/lars.heinitz/node_modules:/Users/node_modules:/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/react@17.0.2/node_modules/react/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/react@17.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/node_modules:/Users/lars.heinitz/Desktop/IPA2022/node_modules:/Users/lars.heinitz/Desktop/node_modules:/Users/lars.heinitz/node_modules:/Users/node_modules:/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/react@17.0.2/node_modules/react/node_modules:/Users/lars.heinitz/Desktop/IPA2022/LOCAL_BACKUP/10_zukünftige_Weiterentwicklung/ipa-manual/node_modules/.pnpm/react@17.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
